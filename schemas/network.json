{
  "$id": "https://fildev.network/schemas/network.json",
  "type": "object",
  "properties": {
    "Name": {
      "type": "string",
      "examples": [
        "calibration"
      ]
    },
    "Purpose": {
      "type": "string",
      "examples": [
        "For miners to prepare for the Space Race (recommended for most miners)"
      ]
    },
    "Maintainer": {
      "type": "string",
      "examples": [
        "Protocol Labs"
      ]
    },
    "Stability": {
      "type": "string",
      "enum": ["low", "moderate", "high"],
      "examples": [
        "moderate"
      ]
    },
    "Status": {
      "type": "string",
      "enum": ["online", "offline", "unstable"],
      "examples": [
        "online"
      ]
    },
    "NetworkParameters": {
      "type": "object",
      "properties": {
        "SupportedProofTypes": {
          "type": "array",
          "items": {
            "type": "object",
            "properties": {
              "Name": {
                "type": "string",
                "examples": [
                  "RegisteredSealProof_StackedDrg512MiBV1"
                ]
              },
              "Size": {
                "type": "string",
                "examples": [
                  "536870912"
                ]
              },
              "RegisteredSealProof": {
                "type": "integer",
                "examples": [
                  2
                ]
              }
            },
            "required": []
          }
        },
        "ConsensusMinerMinPower": {
          "type": "string",
          "examples": [
            "2147483648"
          ]
        },
        "EpochDurationSeconds": {
          "type": "integer",
          "examples": [
            25
          ]
        },
        "ExpectedLeaderPerEpoch": {
          "type": "integer",
          "examples": [
            5
          ]
        },
        "WPoStProvingPeriod": {
          "type": "integer",
          "examples": [
            3456
          ]
        },
        "WPoStChallengeWindow": {
          "type": "integer",
          "examples": [
            72
          ]
        },
        "WPoStPeriodDeadlines": {
          "type": "integer",
          "examples": [
            48
          ]
        },
        "PreCommitChallengeDelay": {
          "type": "integer",
          "examples": [
            150
          ]
        }
      },
      "required": []
    },
    "ResourceLinks": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "Name": {
            "type": "string",
            "examples": [
              "Faucet"
            ]
          },
          "Link": {
            "type": "string",
            "examples": [
              "https://faucet.calibration.fildev.network/"
            ]
          }
        },
        "required": []
      }
    },
    "Git": {
      "type": "object",
      "properties": {
        "Commit": {
          "type": "string",
          "examples": [
            "477dd536b5b5824a411c24a710af8f8550eb8fdc"
          ]
        },
        "Branch": {
          "type": "string",
          "examples": [
            "ntwk-calibration"
          ]
        },
        "Tag": {
          "type": "string",
          "examples": [
            "ntwk-calibration-7.29.0"
          ]
        }
      },
      "required": []
    },
    "Genesis": {
      "type": "object",
      "properties": {
        "Car": {
          "type": "object",
          "properties": {
            "/": {
              "type": "string",
              "examples": [
                "QmU6bW4vBFYB74EStK3DqwEPkYYunn1HD5FKZas5deowWA"
              ]
            }
          },
          "required": []
        },
        "Timestamp": {
          "type": "string",
          "examples": [
            "2020-07-29T02:26:00Z"
          ],
          "format": "date-time"
        },
        "BlockZeroCid": {
          "type": "string",
          "examples": [
            "bafy2bzacedm25s7nmp4forvpbian3ik36uv4yik2fe7tqzgv6z6xystmi3i5q"
          ]
        },
        "Sha1Digest": {
          "type": "string",
          "examples": [
            "dd1b6f30dc85e2f2f39c24265c41ff735b8a156e"
          ]
        }
      },
      "required": []
    },
    "BootstrapPeers": {
      "type": "array",
      "items": {
       "type": "string",
        "examples": [
          "/dns4/bootstrap-0.calibration.fildev.network/tcp/1347/p2p/12D3KooWBEDQ5Xwh3JC67yxjNf91pZcpavrAwaqprNzbquC1yj6t"
        ]
      }
    }
  },
  "required": []
}
